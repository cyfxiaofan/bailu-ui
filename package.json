{
  "name": "bailu-ui",
  "version": "0.1.42-7",
  "type": "module",
  "license": "MIT",
  "main": "./dist/bailu-ui.umd.js",
  "module": "./dist/bailu-ui.es.js",
  "types": "./dist/typings/index.d.ts",
  "unpkg": "./dist/bailu-ui.umd.js",
  "exports": {
    ".": {
      "import": "./dist/bailu-ui.es.js",
      "require": "./dist/bailu-ui.umd.js",
      "types": "./dist/typings/index.d.ts"
    },
    "./css": "./dist/style.css",
    "./components/*": "./dist/components/*",
    "./locale/*": "./dist/locale/*"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vite serve --host 0.0.0.0  --port=10010",
    "docs:dev": "pnpm register:components && vitepress dev docs",
    "docs:build": "pnpm register:components && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "pub": "npm version prerelease && pnpm build:helper && npm publish",
    "build:helper": "node build/build-helper.cjs",
    "build:types": "tsc",
    "build:lang": "rollup --config --configPlugin typescript --bundleConfigAsCjs",
    "build": "vue-tsc --noEmit && vite build",
    "dist": "gulp -f build/build.ts",
    "register:components": "vitepress-rc",
    "check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/gulp": "^4.0.10",
    "@types/gulp-autoprefixer": "^0.0.34",
    "@types/gulp-sass": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-typescript": "^9.1.0",
    "components-helper": "^2.1.4",
    "core-js": "^3.8.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "flexsearch": "^0.7.31",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-sass": "^5.1.0",
    "markdown-it": "^13.0.1",
    "rollup": "^3.23.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "sucrase": "^3.32.0",
    "typescript": "~4.5.5",
    "unplugin-vue-define-options": "^1.3.5",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vitepress": "1.0.0-alpha.30",
    "vitepress-plugin-search": "1.0.4-alpha.20",
    "vitepress-theme-demoblock": "^2.0.2",
    "vue": "3.2.44",
    "vue-tsc": "^1.6.5"
  },
  "vetur": {
    "tags": "./dist/json/tags.json",
    "attributes": "./dist/json/attributes.json"
  },
  "web-types": "./dist/json/web-types.json"
}
